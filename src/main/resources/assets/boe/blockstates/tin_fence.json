{
  "multipart": [
    { "apply": { "model": "boe:tin_fence_post" } },
    { "when": { "north": "true" },
      "apply": { "model": "boe:tin_fence_side", "y": 0, "uvlock": true}
    },
    { "when": { "east": "true" },
      "apply": { "model": "boe:tin_fence_side", "y": 90, "uvlock": true}
    },
    { "when": { "south": "true" },
      "apply": { "model": "boe:tin_fence_side", "y": 180, "uvlock": true}
    },
    { "when": { "west": "true" },
      "apply": { "model": "boe:tin_fence_side", "y": 270, "uvlock": true}
    }
  ]
}
